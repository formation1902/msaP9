    },
    {
      "type":       "blob",
      "name":       "categoryRS",
      "dataType":   "binary",
      "path":       "an-existing-container/category_top_articles.csv",
      "connection": "AzureWebJobsStorage",
      "direction":  "in"
    }
  ]
