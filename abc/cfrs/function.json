{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "userRequest",
      "methods": ["post"]
    },
    {
      "type":       "blob",
      "name":       "cfrsFile",
      "dataType" :  "binary",
      "path":       "an-existing-container/cfrs_predictions.csv",
      "connection": "AzureWebJobsStorage",
      "direction":  "in"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}