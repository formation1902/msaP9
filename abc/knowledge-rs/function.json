      "direction": "in",
      "name": "userRequest",
      "methods": ["post"]
    },
    {
      "type":       "http",
      "direction":  "out",
      "name":       "$return"
    },
    {
      "type":       "blob",
      "name":       "regionRS",
      "dataType" :  "binary",
      "path":       "an-existing-container/region_top_articles.csv",
